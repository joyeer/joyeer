func twoSum(nums:[Int], target: Int): [Int] {
    var dict = [Int:Int]()
    var length = nums.size()
    var index = 0
    while index < length  {
        var num = nums[index]
        dict[num] = index
        index = index + 1
    }

    index = 0
    while index < length {
        var num = target - nums[index]
        var found = dict[num]
        if found != nil && found != index {
            return [index, found]
        }

        index = index + 1
    }

    return [-1, -1]
}

var sums = [12, 24, 11, 9, 7]

var result = twoSum(nums: sums, target: 22)
print(message: result)
